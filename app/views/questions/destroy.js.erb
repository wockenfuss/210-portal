$('#questionContainer').html("");
$('#questionContainer').append('<%= j render :partial => "questions/index", :locals => { :questions => @questions } %>');

if ( $('#addQuestionForm form').is(':visible') ) {
	myApp.clearForm($('#addQuestionForm form'));
	$('#addQuestionForm').slideToggle();
} else {
	myApp.Questions().toggleDelete();
}
myApp.Links('#linkbarLinks a').enableAll();