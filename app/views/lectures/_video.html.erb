<div class="<%= section['class_name'] %>" >
	<div class="videoContainer">
		<div class="column firstColumn">
			<p>
				<%= section['text'].html_safe %> 
			</p>
		</div>
		<div class="column secondColumn">
			<% section['content'].each do |content| %>
				<div class="videoWrapper">
					<%= content['video'] ? content['video'].html_safe : image_tag(content['image']) %>
				</div>
			<% end %>
		</div>
	</div>
</div>